<UserControl x:Class="QTRHacker.Views.PagePanels.DirectFunctionsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QTRHacker.Views.PagePanels"
             mc:Ignorable="d" 
             Width="300" Height="340">
    <Grid>
        <TabControl x:Name="FunctionsTab" TabStripPlacement="Left" ItemsSource="{Binding TabItems}">
        </TabControl>
    </Grid>
</UserControl>
